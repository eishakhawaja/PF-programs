#include<stdio.h>



int main()
{
int hours, pay, wage;
int count = 1;



while(count<=7)
{



printf("Enter no. of hours worked\n");
scanf("%d",&hours);



printf("Enter wage per hour\n");
scanf("%d",&wage);



pay = hours * wage;



printf("The salary of employee # %d is %d\n\n",count, pay);



count++;
}







return 0;
}
